<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
    <Appenders>
        <Console name="stdout">
            <PatternLayout
                pattern="%d{yyyy-MM-dd HH:mm:ss} [%thread] %-5level %logger{35} - %msg%n" />
        </Console>
    </Appenders>
    <Loggers>
        <Root level="debug">
            <AppenderRef ref="stdout" />
        </Root>
    </Loggers>
</Configuration>